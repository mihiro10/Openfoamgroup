FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      sample;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

type sets;
libs            ("libsampling.so");

interpolationScheme cell;

fields          ( p U );

setFormat       raw;

sets
(
    y_dir 
    {
        type    lineCell;
        axis    xyz;
        start   ( 0.05 -0.01 0.005);
        end     ( 0.05 0.11 0.005);
    }
);


